{
  "name": "@tinacms/starter",
  "version": "0.0.2",
  "private": true,
  "scripts": {
    "dev": "tinacms dev -c \"next dev\"",
    "build": "tinacms build && next build && node ./scripts/sitemap.mjs",
    "start": "tinacms build && next start",
    "export": "npm run build && next export",
    "proxy": "local-ssl-proxy --source 3001 --target 3000",
    "lint": "eslint . --ext .ts,.tsx",
    "cypress": "cypress open --browser chrome --e2e",
    "vrt:base": "cypress run --browser chrome --e2e --env type=base baseUrl=http://localhost:3000 --spec cypress/e2e/vrt/*",
    "vrt:actual": "cypress run --browser chrome --e2e --env baseUrl=http://localhost:3000 --spec cypress/e2e/vrt/*"
  },
